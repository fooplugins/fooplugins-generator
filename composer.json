{
    "name": "fooplugins/generator",
    "description": "Generate WordPress Plugins In Seconds",
    "type": "library",
    "require": {
        "composer/installers": "v1.0.12"
    },
    "autoload"    : {
        "psr-4" : {
            "FooPlugins\\" : "includes"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "license": "GPL 3.0",
    "authors": [
        {
            "name": "FooPlugins",
            "email": "info@fooplugins.com"
        }
    ]
}
